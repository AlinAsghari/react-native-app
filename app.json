{
  "expo": {
    "name": "ReactNativeExample",
    "slug": "ReactNativeExample",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/logo-red.png",
    "splash": {
      "image": "./app/assets/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "ir.bimcoluder.reactnativeexample",
      "buildNumber": "1.0.0",      
      "supportsTablet": true
    },
    "android": {
      "package": "ir.bimcoluder.reactnativeexample",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "this app will be the first application of our team..." ,
    "notification":{
      "iosDisplayInForeground" : true
    }
  }
}
